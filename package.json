{
  "name": "nodejs-es6-starter",
  "version": "0.0.2",
  "description": "Jose Miranda's NodeJS ES6 Starter",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node index.js",
    "build": "rimraf ./build && babel ./ --out-dir ./build --ignore ./node_modules,./build,package.json,.package-lock.json,.babelrc"
  },
  "keywords": [],
  "author": "Jose S. Miranda III",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "nodemon": "^2.0.0",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^3.2.0"
  }
}
